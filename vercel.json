{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python@3.9.16"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/health",
      "dest": "/api/app.py"
    },
    {
      "src": "/",
      "dest": "/api/app.py"
    }
  ]
}