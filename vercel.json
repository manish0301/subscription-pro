{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/health",
      "dest": "/main.py"
    },
    {
      "src": "/merchant",
      "dest": "/main.py"
    },
    {
      "src": "/",
      "dest": "/main.py"
    }
  ]
}