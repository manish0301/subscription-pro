{
  "functions": {
    "backend/subscription-api/api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/subscription-api/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/user-portal/dist/$1"
    }
  ]
}