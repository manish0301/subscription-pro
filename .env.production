# Production Environment Variables
# DO NOT commit this file to version control
# Copy to .env and update with actual production values

# Database Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-anon-key-here

# Security Configuration
JWT_SECRET_KEY=your-production-jwt-secret-minimum-32-characters
SECRET_KEY=your-production-secret-key-minimum-32-characters

# Payment Gateway Configuration (Production)
RAZORPAY_KEY_ID=rzp_live_your_production_key
RAZORPAY_KEY_SECRET=your_production_secret

# CORS Configuration (Production domains only)
CORS_ORIGINS=https://your-user-portal.vercel.app,https://your-admin-portal.vercel.app

# Application Configuration
FLASK_ENV=production
VERCEL_ENV=production

# Monitoring and Logging
SENTRY_DSN=your-sentry-dsn-for-error-tracking

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=60

# Security Headers
SECURITY_HEADERS_ENABLED=true
